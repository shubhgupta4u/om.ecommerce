        "Period": "5s",
        "PeriodTimespan": 5,
        "Limit": 50
      },
      "LoadBalancerOptions": {
        "Type": "LeastConnection"
      },
      "QoSOptions": {
        "ExceptionsAllowedBeforeBreaking": 3,
